{
  "name": "hz-map-tools",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "npm run git-commit && vite build",
    "preview": "vite preview",
    "lint:script": "eslint --ext .js,.jsx,.vue, --fix --quiet ./",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:style": "stylelint --fix \"src/**/*.{css,scss}\"",
    "prepare": "husky install",
    "git-commit": "node git.commit.mjs"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "npm run lint",
      "git add ."
    ],
    "**/*.{scss,css}": [
      "npm run lint:style",
      "git add ."
    ]
  },
  "dependencies": {
    "@mapbox/geojsonhintesm": "^3.1.8",
    "@maptalks/gl": "^0.80.0",
    "@maptalks/gl-layers": "^0.17.0",
    "@maptalks/vt": "^0.78.2",
    "@terraformer/wkt": "^2.1.2",
    "@turf/helpers": "^6.5.0",
    "axios": "^1.2.1",
    "babel-eslint": "^10.1.0",
    "chalk": "^5.2.0",
    "element-plus": "^2.2.27",
    "file-saver": "^2.0.5",
    "is-wkt": "^0.0.1",
    "jsoneditor": "^9.9.2",
    "jszip": "^3.10.1",
    "lodash-es": "^4.17.21",
    "maptalks": "^1.0.0-rc.17",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.28",
    "rollup-plugin-compress-dist": "^1.1.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.57.1",
    "stylelint-prettier": "^2.0.0",
    "unplugin-icons": "^0.15.1",
    "vite": "^4.1.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.3.0-alpha.4",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@iconify-json/ep": "^1.1.8",
    "@rushstack/eslint-patch": "^1.1.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "commitlint": "^17.3.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.7.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vite-plugin-stylelint": "^4.1.4"
  }
}
